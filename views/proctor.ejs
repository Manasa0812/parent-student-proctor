  <style type="text/css">
    ul{
      position: relative;
      padding: 0;
      margin: 0;
      display: flex;
    }
    ul .a{
      color: white;
      position: relative;
      list-style: none;
      width: 60px;
      height: 60px;
      margin: 0 30px;
      transform: rotate(-30deg) skew(25deg);
      background: #ccc;
    }
    ul .a span{
      position: absolute;
      top:0;
      left: 0;
      width: 100%;
      height: 100%;
      background: #000;
      transition: 0.5s;
      display: flex !important;
      align-items: center;
      justify-content: center;
      color: #fff;
      font-size: 30px !important;
    }
    ul .a:hover span:nth-child(5){
      transform: translate(40px,-40px);
      opacity: 1;
    }
    ul .a:hover span:nth-child(4){
      transform: translate(30px,-30px);
      opacity: .8;
    }
    ul .a:hover span:nth-child(3){
      transform: translate(20px,-20px);
      opacity: .6;
    }
    ul .a:hover span:nth-child(2){
      transform: translate(10px,-10px);
      opacity: .4;
    }
    ul .a:hover span:nth-child(1){
      transform: translate(0,0);
      opacity: .2;
    }
ul .a:nth-child(1) span{
  background: #3b5999;
}
ul .a:nth-child(2) span{
  background: #55acee;
}
ul .a:nth-child(3) span{
  background: #dd4b39;
}
ul .a:nth-child(4) span{
  background: #0077B5;
}
ul .a:nth-child(5) span{
  background: #e4405f;
}
#heading{
   position: relative;
        /* position: absolute;*/
  /*top:80%;*/
  margin-top: 150px;
  cursor: pointer;
  border: 1px solid black;
  border-radius: 10px;
  padding: 20px;
  box-shadow: 10px 20px 20px black;
  left:50%;
  transform: translate(-50%,-50%);
  /*transform: translate(-50%,-50%);*/
  color: #009999;
  text-shadow:5px 5px 5px #00A1BF ;
}
.nav-tabs
 {
   border-color:#fff;
  /* width:60%;*/
 }

.nav-tabs > .d a { 
    border: 1px solid #009999;
     background-color:#009999 ; 
    /*background-color:#00A1BF !important; */
    color:#fff;
    }

.nav > .d .active > a,
.nav > .d .active > a:focus,
.nav > .d .active > a:hover{

    background-color: #00A1BF;
   color:#fff;
   border: 1px solid black;
    border-bottom-color: transparent;
    }

.nav-tabs > .d > a:hover{
  background-color:  #00A1BF !important;
    border-radius: 5px;
    color:#000;
} 

.nav-tabs > .d > a >li:hover{
  background-color:  #00A1BF !important;
    border-radius: 5px;
    color:#000;
} 

.tab-pane {
  margin-left: 90px;
    margin-top: 30px; 
