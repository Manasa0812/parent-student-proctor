    .tabContain{
      width: 100%;
      height: 1250px;
    }
    .tabContain .buttonContain{
      height: 15%;
    }
    .tabContain .buttonContain button{
      width: 20%;
      height: 50%;
      float: left;
      border: none;
      outline: none;
      cursor: pointer;
      padding: 10px;
      font-family: sans-serif;
      font-size: 10px;
      background-color:#00A1BF;
    }
    .tabContain .buttonContain button:hover{
      color: white;
      /*background: linear-gradient(to right, #3499FF, #3A3985);*/
      /*border-radius: 30%;*/
    }
    .tabContain .tabPanel{
      height: 85%;
     /* background-color: grey;*/
    /* background: linear-gradient(to right, #3499FF, #3A3985);*/
      background-color: #00A1BF;
      color: white;
      text-align: center;
      padding-top: 105px;
      box-sizing: border-box;
      font-family: sans-serif;
      font-size: 22px;
      display: none;
    }
    .main_part{
      margin:0px;
      padding:0px;
      display:flex;
      justify-content:center;
      align-items: center;
      min-height: 100vh;
      font-family: 'poppins'.sans-serif;
    }
    .con{
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
    }
    .con .box{
      position: relative;
      width:350px;
      padding: 40px;
      background: #fff;
      border: 2px solid #00A1BF;
      /*box-shadow: 0 5px 15px rgba(0,0,0,.1);*/
       box-shadow: -30px 30px 20px rgba(0,0,0,0.3);
      border-radius: 4px;
      margin: 20px;
     /* margin-left: 400px;*/
      box-sizing: border-box;
      overflow: hidden;
      text-align: center;
    }
    .con .box .icon{
      position: relative;
      width: 120px;
      height: 70px;
      color: #fff;
      background: #000;
      justify-content: center;
      align-items: center;
      margin: 0 auto;
      padding: 20px;
      border-radius: 20%;
      font-size: 15px;
      font-weight: 700;
      transition: 1s;
    }
    .con .box:nth-child(1) .icon{
      box-shadow: 0 0 0  #00A1BF;
      background:  #00A1BF;
    }
    .con .box:nth-child(1):hover .icon{
      box-shadow: 0 0 0 400px  #00A1BF;
    }
   
  .con .box .content{
      position: relative;
      z-index:1;
      transition: 0.5s;
    }
    .con .box:hover .content{
      color:#fff; 
    }
    .con .box .content h2{
    font-size: 20px;
    margin: 10px 0;
    padding: 0;
    }

/*end of  feedback model button -----------------------------------------------------------------------------------------------------------------------------------------*/



